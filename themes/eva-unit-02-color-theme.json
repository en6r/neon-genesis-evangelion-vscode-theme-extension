 "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"foreground": "#33ce52", "fontStyle": "italic"}
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {"foreground": "#ed2323", "fontStyle": "bold"}
    },
    {
      "name": "Constants and Numbers",
      "scope": ["constant.numeric", "constant.language", "constant.other.color"],
      "settings": {"foreground": "#7e7b7b"}
    },
    {
      "name": "String, Markup Changed",
      "scope": ["string", "markup.changed"],
      "settings": {"foreground": "#ffffff"}
    },
    {
      "name": "General Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#ed2323"
      }
    },
    {
      "name": "Other Variables",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#ed2323"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": ["entity.name.function", "support.function", "variable.function", "entity.name.method"],
      "settings": {
        "foreground": "#ed2323"
      }
    },
    {
      "name": "Function Arguments",
      "scope": ["variable.parameter"],
      "settings": {"foreground": "#7e7b7b"}
    },
    {
      "name": "Class Names",
      "scope": ["entity.name.class", "support.class"],
      "settings": {
        "foreground": "#f99d52"
      }
    },
    {
      "name": "Type Definitions",
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#f99d52"
      }
    },
    {
      "name": "Namespaces",
      "scope": [
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#f99d52"
      }
    },
    {
      "name": "Punctuation and Operators",
      "scope": ["punctuation", "meta.tag", "punctuation.definition.tag", "punctuation.separator", "keyword.other.template", "keyword.other.substitution"],
      "settings": {"foreground": "#7e7b7b"}
    },
    {
      "name": "Invalid and Errors",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {"foreground": "#b8cd1d", "fontStyle": "underline"}
    },
    {
      "name": "Markup Inserted",
      "scope": ["markup.inserted"],
      "settings": {"foreground": "#f99d52"}
    },
    {
      "name": "Markup Deleted",
      "scope": ["markup.deleted"],
      "settings": {"foreground": "#ed2323"}
    },
    {
      "name": "HTML and CSS Attributes",
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.class", "text.html.basic entity.other.attribute-name"],
      "settings": {"foreground": "#f99d52", "fontStyle": "italic"}
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {"foreground": "#fbe6e5"}
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {"foreground": "#f99d52"}
    },
    {
      "name": "JSON Keys",
      "scope": ["source.json support.type.property-name.json"],
      "settings": {"foreground": "#f99d52"}
    },
    {
      "name": "Markdown - Heading",
      "scope": ["markup.heading", "markup.heading.markdown punctuation.definition.heading.markdown"],
      "settings": {"foreground": "#f99d52", "fontStyle": "bold"}
    },
    {
      "name": "Markdown - Bold",
      "scope": ["markup.bold"],
      "settings": {"foreground": "#ed2323", "fontStyle": "bold"}
    },
    {
      "name": "Markdown - Italic",
      "scope": ["markup.italic"],
      "settings": {"foreground": "#ea8533", "fontStyle": "italic"}
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote"],
      "settings": {"foreground": "#505050", "fontStyle": "italic"}
    },
    {
      "name": "Markdown - Links",
      "scope": ["string.other.link.title.markdown", "constant.other.reference.link.markdown"],
      "settings": {"foreground": "#f99d52", "fontStyle": "underline"}
    }
  ]
}
